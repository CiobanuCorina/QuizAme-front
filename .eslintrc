{
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/recommended"
  ],
  "rules": {
    "vue/multi-word-component-names": ["error", {
      "ignores": ["Dashboard", "Quiz", "Question", "Auth", "Register", "Login"]
    }]
  }
}